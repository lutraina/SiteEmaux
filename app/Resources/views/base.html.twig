								<p>Une technique de peinture .... Une technique de peinture .... Une technique de peinture .... Une technique de peinture ....</p>
							</header>
							<ul class="features">
                                                            {% for produit in produits %}
								<li class="icon fa-paper-plane-o">
									<h3>{{produit.categorie}}</h3>
									<p><img height="200" width="250" src="{{ asset('uploads/'~produit.image) }}" alt="" />
                                                                            <br/></p>
								</li>
							    {% endfor %}	
							</ul>
						</div>
					</section>

				<!-- CTA -->
					<section id="cta" class="wrapper style4">
						<div class="inner">
							<header>
								<h2>Plus d'actualités</h2>
