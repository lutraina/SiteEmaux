							</header>
							<ul class="actions vertical">
								<li><a href="#" class=" "><img width="215" src="{{ asset('uploads/facebook-icon.jpg') }}" alt="" /></a></li>
								<li><a href="#" class="button fit">Panneau vide</a></li>
							</ul> 
						</div>
					</section>
				
				{% endblock %}
				
                                <!-- Contact -->
					<section id="three" class="wrapper style3">
