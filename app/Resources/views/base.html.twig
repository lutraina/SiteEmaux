                                </section>
                                <section class="spotlight">
                                        <div class="image"><img height="400px" src="{{ asset('uploads/'~homepageSections.1.image) }}" alt="" /></div><div class="content">
                                                <h2>{{ homepageSections.1.titre }}<br />
                                                </h2>
                                                <p>{{ homepageSections.1.description }}</p>
                                        </div>
                                </section>
                                <section class="spotlight">
                                        <div class="image"><img height="400px" src="{{ asset('uploads/'~homepageSections.2.image) }}" alt="" /></div><div class="content">
                                                <h2>{{ homepageSections.2.titre }}<br />
                                                </h2>
                                                <p>{{ homepageSections.2.description }}
Virginie, Créatrice</p>
                                        </div>
                                </section>
                                <section class="spotlight">
